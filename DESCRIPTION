Package: calibtools
Title: Posterior Probability Calibration Tools for Machine Learning Algorithms
Version: 0.0.0.9000
Authors@R: 
   c(person("Norm", "Matloff", email = "matloff@cs.ucdavis.edu", role = c("aut", "cre"), comment = c(ORCID = "0000-0001-9179-6785")),
